<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nextpurchases.jrodriguez.mynextpurchases">

    <uses-permission android:name="android.permission.INTERACT_ACROSS_USERS_FULL"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Principal"></activity>
        <activity android:name=".AddSalary"></activity>
        <activity android:name=".Dashboard"></activity>
        <activity android:name=".AddBudget"></activity>
        <activity android:name=".RegisterSpent"></activity>

    </application>



</manifest>
